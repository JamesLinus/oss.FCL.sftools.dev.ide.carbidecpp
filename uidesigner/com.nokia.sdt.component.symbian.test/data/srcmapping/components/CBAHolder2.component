<componentDefinition xmlns="http://www.nokia.com/sdt/emf/component" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<!-- testing version, using "child of type" path to find CBA -->

	<component qualifiedName="com.nokia.sdt.series60.test.CBAHolder2" 
		category="Basic"
		version="0.1">

		<properties>
			<property name="name" type="uniqueName"/>
		</properties>

		<sourceMapping>
  		  <mapResource struct="EIK_APP_INFO" headers="cba.rh">
 		  <select property="[com.nokia.sdt.series60.test.CBA].CBA">
		    <choice value="com.nokia.sdt.series60.test.CBA.Type.CUSTOM">
		    	<!-- emit a reference to the resource generated -->
		        <mapReferenceMember property="[com.nokia.sdt.series60.test.CBA]" member="cba"/>
		    </choice>
		    <choice>
				<!-- else, emit the builtin -->
				<mapIdentifierMember property="[com.nokia.sdt.series60.test.CBA].CBA" member="cba" />
		    </choice> 
		  </select>
		 </mapResource>
		</sourceMapping>
		
	</component>
</componentDefinition>
