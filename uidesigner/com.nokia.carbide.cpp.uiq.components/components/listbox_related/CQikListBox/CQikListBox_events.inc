<events>
	<!--MQikListBoxObserver events-->
	<!--TQikListBoxEvent in MQikListBoxObserver.h-->
	<event name="listBoxEventItemTapped"
		displayName="%itemTapped"
		descriptionKey="itemTappedDescription"
		category="%listbox.category"
		group="Control"
		handlerNameTemplate="Handle{title(control->name)}EEventItemTappedL"/>
