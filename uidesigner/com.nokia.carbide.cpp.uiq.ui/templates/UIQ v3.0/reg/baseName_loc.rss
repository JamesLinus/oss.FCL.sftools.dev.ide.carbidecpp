#include <AppInfo.rh>
#ifdef LANGUAGE_01  // UK English
#include "ViewsPrototype_loc_l01.rls"
#endif
#ifdef LANGUAGE_04  // Spanish
#include "ViewsPrototype_loc_l04.rls"
#endif
#ifdef LANGUAGE_02  // French
#include "ViewsPrototype_loc_l02.rls"
#endif

RESOURCE LOCALISABLE_APP_INFO
	{
	// not currently used by UIQ, but it can be used in the future
	short_caption = STRING_r_ViewsPrototype_short_caption;
	caption_and_icon =
		{
		CAPTION_AND_ICON_INFO
			{
			// The caption text is defined in the rls file
			caption = STRING_r_ViewsPrototype_caption;
			// Icons are used to represent applications in the 
			// application launcher and application title bar.
			// The number_of_icons value identifies how many icons 
			// that exist in the icon_file.
			number_of_icons = 3;
			// Using the application icons.
			icon_file = "\\Resource\\Apps\\ViewsPrototype.mbm";
			}
		};
	}

